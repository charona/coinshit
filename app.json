{
  "expo": {
    "name": "Coinshit",
    "slug": "coinshit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "coinshit",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F7931A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.coinshit.app",
      "associatedDomains": ["applinks:coinshit.app"]
    },
    "android": {
      "package": "com.coinshit.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F7931A"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "coinshit.app",
              "pathPrefix": "/entry"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Coinshit to access your photos to upload product images."
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Allow Coinshit to use your location to detect your currency."
        }
      ]
    ]
  }
}
